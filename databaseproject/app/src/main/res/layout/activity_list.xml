<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    tools:context=".ListActivity">

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="wrap_content"
        android:layout_height="177dp"
        android:layout_marginEnd="300dp"
        android:layout_marginBottom="400dp"
        app:srcCompat="@drawable/a" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="112dp"
        android:layout_height="148dp"
        android:layout_alignWithParentIfMissing="false"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="false"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginVertical="10dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="235dp"
        app:srcCompat="@drawable/b" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="98dp"
        android:layout_height="145dp"
        android:layout_alignWithParentIfMissing="false"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="false"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginEnd="179dp"
        android:layout_marginRight="313dp"
        android:layout_marginBottom="214dp"
        android:adjustViewBounds="false"
        android:baselineAlignBottom="false"
        android:clickable="false"
        android:cropToPadding="false"
        android:duplicateParentState="false"
        android:fadeScrollbars="false"
        app:srcCompat="@drawable/c" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="111dp"
        android:layout_height="180dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="false"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="400dp"
        app:srcCompat="@drawable/d" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="132dp"
        android:layout_height="185dp"
        android:layout_alignWithParentIfMissing="false"
        android:layout_alignParentStart="false"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="false"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="false"
        android:layout_centerInParent="false"
        android:layout_centerHorizontal="false"
        android:layout_marginEnd="147dp"
        android:layout_marginRight="147dp"
        android:layout_marginBottom="179dp"
        app:srcCompat="@drawable/e" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="wrap_content"
        android:layout_height="147dp"
        android:layout_alignWithParentIfMissing="false"
        android:layout_alignRight="@id/ContinueBtn"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="false"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="129dp"
        android:layout_marginLeft="129dp"
        android:layout_marginEnd="164dp"
        android:layout_marginRight="164dp"
        android:layout_marginBottom="181dp"
        app:srcCompat="@drawable/f" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="104dp"
        android:layout_alignParentStart="false"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="false"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="false"
        android:layout_alignParentBottom="true"
        android:text="Hurry Up!!! Flat 50% off on these products"
        android:textAlignment="center" />

    <Button
        android:id="@+id/ContinueBtn"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_alignWithParentIfMissing="false"
        android:layout_above="@+id/textView"
        android:layout_below="@+id/imageView19"
        android:layout_alignStart="@id/imageView19"
        android:layout_alignParentStart="false"
        android:layout_alignParentLeft="false"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="false"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="-15dp"
        android:layout_marginTop="91dp"
        android:layout_marginBottom="11dp"
        android:importantForAccessibility="no"
        android:includeFontPadding="false"
        android:isScrollContainer="false"
        android:keepScreenOn="false"
        android:text="Continue" />

    <Button
        android:id="@+id/FetchBtn"
        android:layout_width="115dp"
        android:layout_height="35dp"
        android:layout_alignWithParentIfMissing="false"
        android:layout_above="@+id/textView"
        android:layout_below="@+id/imageView19"
        android:layout_alignStart="@id/imageView19"
        android:layout_alignParentStart="false"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="false"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="-15dp"
        android:layout_marginTop="91dp"
        android:layout_marginBottom="8dp"
        android:importantForAccessibility="no"
        android:includeFontPadding="false"
        android:isScrollContainer="false"
        android:keepScreenOn="false"
        android:text="Fetchinfo" />
</RelativeLayout>